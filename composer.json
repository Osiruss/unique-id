{
    "name": "russell-kitchen/unique-id",
    "description": "Set a unique ID",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "unique id"
    ],
    "support": {
        "docs": "https://github.com/russell-kitchen/unique-id/blob/master/README.md",
        "issues": "https://github.com/russell-kitchen/unique-id/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "russell-kitchen",
            "homepage": "https://github.com/russell-kitchen"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "russell-kitchen\\unique-id\\": "src/"
        }
    },
    "extra": {
        "name": "Unique ID",
        "handle": "unique-id",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/russell-kitchen/unique-id/master/CHANGELOG.md",
        "class": "russell-kitchen\\unique-id\\UniqueId"
    }
